pipline{
    agent none
    stages{
        stage('code checkout'){
        agent{label 'master'}
           steps{
              script{
                  sh "echo hi hello"
              }
           } 
        }
        stage('Build'){
        agent{label 'master'}
           steps{
              script{
                  sh "echo hi hello"
              }
           } 
        }
        stage('Test'){
        agent{label 'master'}
           steps{
              script{
                  sh "echo hi hello"
              }
           } 
        }
        stage('Deploy'){
        agent{label 'master'}
           steps{
              script{
                  sh "echo hi hello"
              }
           } 
        }
        stage('Release'){
        agent{label 'master'}
           steps{
              script{
                  sh "echo hi hello"
              }
           } 
        }
    } 
}     
