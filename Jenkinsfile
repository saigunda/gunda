pipeline {
    agent none
    stages{
        stage('codecheckout'){
			agent { label 'master'}
            steps {
                script{
                    sh "echo hi hello how are you"
                    
                }
            }
        } 
        stage('build'){
		agent { label 'master'}
		steps {
                script{
                    sh "echo hi hello how are you"
                    
                }
            }
        }
		stage('test'){
		agent { label 'master'}
		steps {
                script{
                    sh "echo hi hello how are you"
                    
                }
            }
        }
		stage('deploy'){
		agent { label 'master'}
		steps {
                script{
                    sh "echo hi hello how are you"
                    
                }
            }
        }
		stage('release'){
		agent { label 'master'}
		steps {
                script{
                    sh "echo hi hello how are you"
                    
                }
            }
        }
		
    }
}
